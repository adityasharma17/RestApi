<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Servers.ListSrervers.DetailsServerActivity">

    <TextView
        android:id="@+id/tv8"
        android:layout_width="295dp"
        android:layout_height="38dp"
        android:layout_alignEnd="@+id/tokenDebugView"
        android:layout_below="@+id/tv7"
        android:text="TextView" />

    <TextView
        android:id="@+id/tv17"
        android:layout_width="40dp"
        android:layout_height="38dp"
        android:layout_alignParentEnd="true"
        android:layout_below="@+id/tv13"
        android:text="TextView" />

    <TextView
        android:id="@+id/tv16"
        android:layout_width="105dp"
        android:layout_height="38dp"
        android:layout_alignTop="@+id/tv17"
        android:layout_toEndOf="@+id/tv14"
        android:text="TextView" />

    <TextView
        android:id="@+id/tv15"
        android:layout_width="70dp"
        android:layout_height="38dp"
        android:layout_alignTop="@+id/tv17"
        android:layout_toEndOf="@+id/tv16"
        android:text="TextView" />

    <TextView
        android:id="@+id/tv14"
        android:layout_width="wrap_content"
        android:layout_height="38dp"
        android:layout_alignParentStart="true"
        android:layout_alignTop="@+id/tv17"
        android:text="TextView" />

    <TextView
        android:id="@+id/tv13"
        android:layout_width="295dp"
        android:layout_height="29dp"
        android:layout_alignEnd="@+id/tokenDebugView"
        android:layout_below="@+id/tv12"
        android:text="TextView" />

    <TextView
        android:id="@+id/tv12"
        android:layout_width="295dp"
        android:layout_height="38dp"
        android:layout_alignEnd="@+id/tokenDebugView"
        android:layout_below="@+id/tv11"
        android:text="TextView" />

    <TextView
        android:id="@+id/tv11"
        android:layout_width="295dp"
        android:layout_height="38dp"
        android:layout_alignEnd="@+id/tokenDebugView"
        android:layout_below="@+id/tv10"
        android:text="TextView" />

    <TextView
        android:id="@+id/tv10"
        android:layout_width="295dp"
        android:layout_height="38dp"
        android:layout_alignEnd="@+id/tokenDebugView"
        android:layout_below="@+id/tv9"
        android:text="TextView" />

    <TextView
        android:id="@+id/tv9"
        android:layout_width="295dp"
        android:layout_height="38dp"
        android:layout_alignEnd="@+id/tokenDebugView"
        android:layout_below="@+id/tv8"
        android:text="TextView" />

    <TextView
        android:id="@+id/tv6"
        android:layout_width="295dp"
        android:layout_height="38dp"
        android:layout_alignEnd="@+id/tokenDebugView"
        android:layout_below="@+id/tv5"
        android:text="TextView" />

    <TextView
        android:id="@+id/tv7"
        android:layout_width="295dp"
        android:layout_height="38dp"
        android:layout_alignEnd="@+id/tokenDebugView"
        android:layout_below="@+id/tv6"
        android:text="TextView" />

    <TextView
        android:id="@+id/tv1"
        android:layout_width="295dp"
        android:layout_height="38dp"
        android:layout_alignEnd="@+id/tokenDebugView"
        android:layout_alignParentTop="true"
        android:layout_marginTop="66dp"
        android:text="TextView" />

    <TextView
        android:id="@+id/tokenDebugView"
        android:layout_width="395dp"
        android:layout_height="40dp"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:text="TextView"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="51dp" />

    <TextView
        android:id="@+id/tv4"
        android:layout_width="295dp"
        android:layout_height="38dp"
        android:layout_alignEnd="@+id/tokenDebugView"
        android:layout_below="@+id/tv3"
        android:text="TextView" />

    <TextView
        android:id="@+id/tv3"
        android:layout_width="295dp"
        android:layout_height="38dp"
        android:layout_alignEnd="@+id/tokenDebugView"
        android:layout_below="@+id/tv2"
        android:text="TextView" />

    <TextView
        android:id="@+id/tv5"
        android:layout_width="295dp"
        android:layout_height="38dp"
        android:layout_alignEnd="@+id/tokenDebugView"
        android:layout_below="@+id/tv4"
        android:text="TextView" />

    <TextView
        android:id="@+id/tv2"
        android:layout_width="295dp"
        android:layout_height="38dp"
        android:layout_alignEnd="@+id/tokenDebugView"
        android:layout_below="@+id/tv1"
        android:text="TextView" />

    <ImageView
        android:id="@+id/imageViewStatus"
        android:layout_width="50dp"
        android:layout_height="26dp"
        android:layout_alignTop="@+id/tv17"
        android:layout_toEndOf="@+id/tv15"
        app:srcCompat="@drawable/ic_launcher_foreground" />

    <Button
        android:id="@+id/btnStart"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/tv4"
        android:layout_alignStart="@+id/tokenDebugView"
        android:text="Start" />

    <Button
        android:id="@+id/btnStop"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/tv6"
        android:layout_alignStart="@+id/tokenDebugView"
        android:text="Stop" />

    <Button
        android:id="@+id/btnPause"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_alignStart="@+id/tokenDebugView"
        android:layout_marginBottom="250dp"
        android:text="Pause" />

    <Button
        android:id="@+id/btnAccess"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_alignStart="@+id/tokenDebugView"
        android:layout_marginBottom="168dp"
        android:text="Access" />

    <ProgressBar
        android:id="@+id/progressBar2"
        style="?android:attr/progressBarStyle"
        android:layout_width="wrap_content"
        android:layout_height="29dp"
        android:layout_alignTop="@+id/tv17"
        android:layout_toEndOf="@+id/imageViewStatus"
        android:visibility="gone" />

</RelativeLayout>